config_version=5

[application]

config/name="TwoTime"
run/main_scene="res://menu/menu.tscn"
config/features=PackedStringArray("4.2", "GL Compatibility")
boot_splash/bg_color=Color(0, 0, 0, 1)
config/icon="res://game_art/frog.png"

[autoload]

DialogueManager="*res://addons/dialogue_manager/dialogue_manager.gd"
InteractionManager="*res://interaction/interaction_manager.gd"
SaveSystem="*res://addons/save_system/save_system.gd"
SceneManager="*res://addons/scene_manager/SceneManager.tscn"

[debug]

file_logging/enable_file_logging=true
file_logging/log_path="res://logging.godot.log"
file_logging/max_log_files=1

[display]

